BEGIN PAUP;
  ASSUME wtset=mywts ancstates=ancestor;
  set criterion=parsimony maxtrees=100 increase=no;
  hsearch start=stepwise addseq=random nreps=25 swap=tbr;
  filter best=yes;
  set maxtrees=100 increase=no;
  hsearch start=current swap=tbr hold=1 nbest=100;
  filter best=yes;
  savetrees file=out_wp.trees replace=yes format=newick;
  pscores /scorefile=wp_scores.txt replace=yes;
  contree /majrule treeFile=tmp_wp_con.trees replace=yes semistrict=no showTree=no strict=no;
  QUIT;
END;
